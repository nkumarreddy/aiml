CREATE DATABASE MOVIES;
USE MOVIES;
CREATE TABLE ACTOR(
ACT_ID INT PRIMARY KEY,
ACT_NAME VARCHAR(20),
ACT_GENDER CHAR(6));
DESC ACTOR;

CREATE TABLE DIRECTOR(
DIR_ID INT PRIMARY KEY,
DIR_NAME VARCHAR(20),
DIR_PHONE BIGINT);
DESC DIRECTOR

CREATE TABLE MOVIES(
MOV_ID INT PRIMARY KEY,
MOV_TITLE VARCHAR(20),
MOV_YEAR INT,
MOV_LANG VARCHAR(20),


